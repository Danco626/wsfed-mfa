extends layout

block content
  .w3-card-4
    h1 Welcome #{user.nickname}
    img(src=user.picture)
    h2 Enter OTP sent over email
    pre
      if userProfile
        form(
          method='post'
          action='/mfa'
				)
          input(
            type='text'
            name='mfa_code'
          )
      else
        code #{userProfile}
